<openscam>
  <!-- Note, all units are in mm regardless of 'units' option. -->
  <!-- NC Files -->
  <nc-files>
    heart.ngc cutout.ngc
  </nc-files>

  <!-- Renderer -->
  <resolution v='0.253321'/>
  <resolution-mode v='HIGH'/>

  <!-- Workpiece -->
  <automatic-workpiece v='false'/>
  <workpiece-max v='(40,40,0)'/>
  <workpiece-min v='(-40,-40,-12.7)'/>

  <tool_table>
    <tool length='0.625' number='1' radius='0.05' shape='BALLNOSE' units='INCH'/>
    <tool length='1' number='2' radius='0.03125' shape='CYLINDRICAL' units='INCH'/>
  </tool_table>
</openscam>